{{define "title"}}{{.Title}}{{end}}
{{define "head"}}
<link rel="stylesheet" href="/static/css/cards.css"/>
{{end}}
{{define "main"}}
<div id="cardbox">
  {{range .Animals}}
  <a class="card" href="{{.ID}}">
    <img src="{{.Image}}" alt="{{.Name}} image"/>
    <h2>{{.Name}}</h2>
  </a>
  {{end}}
</div>
{{end}}
